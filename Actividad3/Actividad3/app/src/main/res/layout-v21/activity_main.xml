            android:autoText="false"
            android:textAlignment="center"
            android:gravity="center" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="foto22"
            android:id="@+id/foto22"
            android:padding="5dp"
